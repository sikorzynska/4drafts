@model List<ThreadsBrowseModel>
@{
    ViewBag.Title = "Favourites";
    var user = await this.userManager.GetUserAsync(User);
    var userId = user == null ? null : user.Id;
}
<div class="container-fluid">
    <div class="jumbotron jumbotron-fluid bg-ocean-circles">
        <div class="container">
            <h1 class="display-3 strokeme">@ViewBag.Title</h1>
        </div>
    </div>
    <div id="threads-section">
        <partial name="_ThreadsPartial" model="@Model" />
    </div>
</div>